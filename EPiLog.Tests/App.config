<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="epilog" type="EPiLog.Configuration.EPiLogConfigurationSection, EPiLog" requirePermission="false" />
    </configSections>

    <!-- Use serilog levels: Verbose | Debug | Information | Warning | Error | Fatal -->
    <epilog defaultLevel="Warning">
        <logLevels>
            <add log="default" />
            <add log="verbose" level="Verbose" />
            <add log="debug" level="Debug" />
            <add log="info" level="Information" />
            <add log="warn" level="Warning" />
            <add log="error" level="Error" />
            <add log="fatal" level="Fatal" />
        </logLevels>
    </epilog>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-7.0.0.0" newVersion="7.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>